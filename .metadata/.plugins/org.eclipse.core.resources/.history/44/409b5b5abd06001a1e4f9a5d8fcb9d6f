package com.example.demo.model;

public class EmailImpl {
	private String to;
	private String body;

	public String getTo() {
		return to;
	}

	public void setTo(String to) {
		this.to = to;
	}

	public String getBody() {
		return body;
	}

	public void setBody(String body) {
		this.body = body;
	}

	public EmailImpl(String to, String body) {
		super();
		this.to = to;
		this.body = body;
	}

	public EmailImpl() {
		super();
	}

}
